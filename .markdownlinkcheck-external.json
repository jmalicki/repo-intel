{
  "ignorePatterns": [
    {
      "pattern": "^#"
    },
    {
      "pattern": "^[^/]*\\.md$"
    },
    {
      "pattern": "^\\."
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    },
    {
      "urls": ["https://api.github.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    },
    {
      "urls": ["https://registry.npmjs.org"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    },
    {
      "urls": ["https://pypi.org"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    },
    {
      "urls": ["https://crates.io"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    }
  ],
  "timeout": "30s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "2000ms",
  "aliveStatusCodes": [200, 206, 301, 302]
}
